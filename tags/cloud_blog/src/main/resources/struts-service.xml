<!DOCTYPE struts PUBLIC 
	"-//Apache Software Foundation//DTD Struts Configuration 2.0//EN"
	"http://struts.apache.org/dtds/struts-2.0.dtd">
	
<struts>
	<package name="service" namespace="/web/service" extends="default">
		
		<action name="allservice" class="serviceAction" method="allservice">
			<result name="success">/WEB-INF/pages/service/my_service.jsp</result>
		</action>
		
		<action name="myservice" class="serviceAction" method="myservice">
			<result name="success">/WEB-INF/pages/service/my_service.jsp</result>
		</action>
		
		<action name="makebetter" class="serviceAction" method="makeBetter">
			<result name="input">/WEB-INF/pages/service/make_better.jsp</result>
		</action>
		
		<action name="betterinfo" class="serviceAction" method="BetterInfo">
			<result name="success">/WEB-INF/pages/service/better_info.jsp</result>
		</action>
		
		<action name="deletebetterinfo" class="serviceAction" method="delBetterInfo"/>
	</package>
</struts>